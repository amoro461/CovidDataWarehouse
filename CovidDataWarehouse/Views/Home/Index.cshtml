    let convertCovidVaccineDataIsSucceedElement = document.querySelector("#convertCovidVaccineDataIsSucceedElement");

    function clearDatabase() {
        clearDatabaseIsSucceedElement.innerHTML = "Processing...";

        fetch("/datascraperservices/DeleteDatabaseData")
            .then(response => response.json())
            .then(data => {
                if (data.IsSucceed == true) {
                    clearDatabaseIsSucceedElement.innerHTML = "True";
                }
                else {
                    clearDatabaseIsSucceedElement.innerHTML = "False";
                }


                setTimeout(function () {
                    clearDatabaseIsSucceedElement.innerHTML = "Null";
                }, 100000);
            })
            .catch(exception => { });
    }

