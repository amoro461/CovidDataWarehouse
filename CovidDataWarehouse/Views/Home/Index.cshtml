<body>
    <h1>Covid Data Warehouse Console</h1>
    <div>
        <button onclick="clearDatabase()">Clear Database</button>
        <span>Is Succeed: </span><span id="clearDatabaseIsSucceedElement">Null</span>
    </div>
    <br />
    <div>
        <button onclick="clearDataWarehouse()">Clear Data Warehouse</button>
        <span>Is Succeed: </span><span id="clearDataWarehouseIsSucceedElement">Null</span>
    </div>
    <br />
    <div>
        <button onclick="loadCovidCaseData()">Load AHS Covid Cases Data</button>
        <span>Is Succeed: </span><span id="loadCovidCaseDataIsSucceedElement">Null</span>
    </div>
    <br />
    <div>
        <button onclick="loadCovidVaccineData()">Load AHS Covid Vaccine Data</button>
        <span>Is Succeed: </span><span id="loadCovidVaccineDataIsSucceedElement">Null</span>
    </div>
    <br />
    <div>
        <button onclick="convertCovidCaseData()">Convert Covid Case Data Into Data Warehouse</button>
        <span>Is Succeed: </span><span id="convertCovidCaseDataIsSucceedElement">Null</span>
    </div>
    <br />
    <div>
        <button onclick="convertCovidVaccineData()">Convert Covid Vaccine Data Into Data Warehouse</button>
        <span>Is Succeed: </span><span id="convertCovidVaccineDataIsSucceedElement">Null</span>
    </div>
</body>

<script>
    let clearDatabaseIsSucceedElement = document.querySelector("#clearDatabaseIsSucceedElement");
    let clearDataWarehouseIsSucceedElement = document.querySelector("#clearDataWarehouseIsSucceedElement");
    let loadCovidCaseDataIsSucceedElement = document.querySelector("#loadCovidCaseDataIsSucceedElement");
    let loadCovidVaccineDataIsSucceedElement = document.querySelector("#loadCovidVaccineDataIsSucceedElement");
